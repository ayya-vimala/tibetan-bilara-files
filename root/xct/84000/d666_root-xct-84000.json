{
    "d666:0": "༄༅༅། །རྒྱ་གར་སྐད་དུ། མ་ཧཱ་ག་ན་པ་ཏི་ཏནྟྲ་ནཱ་མ། བོད་སྐད་དུ། ",
    "d666:1": "ཚོགས་ཀྱི་བདག་པོ་ཆེན་པོའི་རྒྱུད་ཅེས་བྱ་བ། ",
    "d666:2": "འཇིག་རྟེན་དབང་ཕྱུག་ལ་ཕྱག་འཚལ་ལོ། །",
    "d666:3": "དེ་ནས་དཔལ་ལྡན་ཚོགས་བདག་ཆེ། །ཕྱག་འཚལ་དཔལ་ལྡན་ཚོགས་བདག་དགྱེས། །གཟུགས་བརྙན་མཎྜལ་གྲུ་བཞིར་བཞག །",
    "d666:4": "ཟླ་བ་དྲུག་ཏུ་མཆོད་པར་བྱ། །",
    "d666:5": "ལྷ་ཡི་འཁོར་ལོས་སྒྱུར་བ་དང་། །ཚེ་འདི་ཉིད་ལ་སྐལ་བ་མཉམ། །",
    "d666:6": "བསོད་ནམས་ཆེན་པོ་ཐོབ་པར་འགྱུར། །",
    "d666:7": "ལོངས་སྤྱོད་ལྷ་དང་མཉམ་པ་དང་། །ཚེ་ཡང་རིང་བའི་མཆོག་ཏུ་འགྱུར། །",
    "d666:8": "མི་ལ་སོགས་པ་དབང་དུ་འགྱུར། །",
    "d666:9": "ལྷ་ཡི་བུ་ཡང་བྲན་བཞིན་འགྱུར། །",
    "d666:10": "བདག་གི་བསྒོ་བ་ཉན་པར་འགྱུར། །",
    "d666:11": "ཀླུ་ཡང་དེ་ཡི་བསྒོ་བ་ཉན། །",
    "d666:12": "བྲན་བཞིན་ལས་རྣམས་བྱེད་པར་འགྱུར། །",
    "d666:13": "ལྷ་ཡི་ལོངས་སྤྱོད་མངའ་རིས་ལས། །འདོད་པའི་ལོངས་སྤྱོད་འབྱུང་བར་འགྱུར། །",
    "d666:14": "དངོས་གྲུབ་སྒྲུབ་པའི་སྐལ་བ་ཅན། །གདུལ་བྱའི་དབང་གི་སྐུ་སྤྲུལ་པ། །ལྷ་ཆེན་ཚོགས་བདག་ཆེན་པོས་བྱ། །",
    "d666:15": "དངོས་གྲུབ་དམ་པ་མཆོག་སྒྲུབ་ན། །",
    "d666:16": "མཐུ་སྟོབས་ཕུན་སུམ་ཚོགས་ལྡན་ཞིང་། །ལོངས་སྤྱོད་ཡིད་བཞིན་འཕེལ་བར་བྱེད། །",
    "d666:17": "འཇིག་རྟེན་ཕན་མཛད་ལྷ་ཡི་ལྷ། །གང་གིས་མཆོད་པ་དངོས་གྲུབ་ཡིན། །",
    "d666:18": "ལྷ་ཆེན་ཚོགས་ཀྱི་བདག་པོའི་རྒྱུད་ལས་དངོས་གྲུབ་འབྱུང་བ་ཞེས་བྱ་བའི་ལེའུ་སྟེ་དང་པོའོ།། །།",
    "d666:19": "དེ་ནས་དབེན་པའི་གནས་སུ་ནི། །དཀྱིལ་འཁོར་གྲུ་བཞི་དག་བྱ་སྟེ། །",
    "d666:20": "བ་ཡི་ལྕི་བས་ལེགས་པར་བྱུག །",
    "d666:21": "འཇམ་ཞིང་མཉེན་ལ་དབུས་མཐོ་བའི། །དབུས་སུ་ཚོགས་བདག་སྐུ་གཟུགས་བཞུགས། །",
    "d666:22": "མེ་ཏོག་དཀར་པོས་དེ་ལ་གཏོར། །",
    "d666:23": "བདག་ཉིད་གཅེར་བུར་བྱུང་བྱས་ཏེ། །",
    "d666:24": "ལོག་འདྲེན་གཟུགས་ཀྱིས་ཡང་དག་མཆོད། །",
    "d666:25": "བ་ར་ཏ་ཀ་ཟླ་བ་དྲུག །ཉིན་རེ་བཞིན་དུ་དུས་གསུམ་དུ། །གསང་སྔགས་བརྒྱ་ནི་རྩ་བརྒྱད་བཟླས། །ཡིད་ལ་རེ་བ་ཐམས་ཅད་རྫོགས། །",
    "d666:26": "བསམ་པ་ཐམས་ཅད་འགྲུབ་པར་འགྱུར། །",
    "d666:27": "ཨོཾ་ནི་བཀྲ་ཤིས་དཔལ་དང་ལྡན། །",
    "d666:28": "གསང་སྔགས་མགོ་ལ་གཞག་པར་བྱ། །",
    "d666:29": "ཡིད་བཞིན་ནོར་བུ་ལྟ་བུ་ཡི། །དངོས་གྲུབ་སྟེར་བྱེད་ཡི་གེ་ཡང་། །དེ་འོག་གྲལ་ལ་གཞག་པར་བྱ། །",
    "d666:30": "ཉི་མ་ལྟ་བུའི་ཡི་གེ་ཡང་། །དེ་ཡི་རྗེས་ལ་བཀླག་པར་བྱ། །",
    "d666:31": "དེ་བཞིན་གཤེགས་པའི་མཚན་དང་པོ། །ཡི་གེ་མཆོག་ནི་བླང་བར་བྱ། །",
    "d666:32": "གསལ་བྱེད་ཡི་གེ་དང་པོ་ཡང་། །དེ་ཡི་མཇུག་ཏུ་བརྗོད་པར་བྱ། །",
    "d666:33": "རིན་ཆེན་ཡི་གེ་སྭཱ་ཞེས་བྱ། །",
    "d666:34": "ལས་ཀྱི་ཡི་གེ་ཧཱ་ཞེས་བྱ། །",
    "d666:35": "དངོས་གྲུབ་འདོད་པས་གསང་སྔགས་བཟླས། །ལྷ་ཆེན་དངོས་གྲུབ་སྒྲུབ་པའི་རྒྱུད་ལས་ལྷའི་དངོས་གྲུབ་ཀྱི་གསང་སྔགས་བསྟན་པའི་ལེའུ་སྟེ་གཉིས་པོའོ།། །།",
    "d666:36": "དེ་ནས་ཚོགས་ཀྱི་བདག་པོ་བཤད། །",
    "d666:37": "ཚོགས་ཀྱི་བདག་པོ་སྐུ་མདོག་དཀར། །གླང་པོ་ཆེ་ཡི་མགོ་བོ་ཅན། །མཆེ་བ་ཡ་གཅིག་ཆག་པ་དང་། །ཕྱག་གཡས་ལ་ཕུག་བསྣམས་པ། བྱི་བའི་གདན་ལ་བཅིབས་པ། རིན་པོ་ཆེས་སྐུ་བརྒྱན་པ། ཟླ་བ་ཚེས་པས་དབུ་བརྒྱན་པ། འོད་གསལ་བ། འོད་ཟེར་འཕྲོ་བ། མདོག་བཟང་བ། ཁ་དོག་བཟང་བ། གསུས་པ་ཆེ་བ། རྒྱན་གྱིས་སྤུད་པའོ། །",
    "d666:38": "དངོས་གྲུབ་སྒྲུབ་པའི་རྒྱུད་ལས་ཚོགས་ཀྱི་བདག་པོ་བསྟན་པའི་ལེའུ་སྟེ་གསུམ་པའོ།། །།",
    "d666:39": "དེ་ནས་སྐར་མ་རྒྱལ་གྱི་ཉིན་པར་མེ་ཏོག་དཀར་པོ་བླངས་ཏེ། ཚོགས་ཀྱི་བདག་པོའི་སྐུ་གཟུགས་བྲིས་ཏེ། ཁ་དོག་དཀར་པོ་གླང་པོ་ཆེའི་དབུ་ཅན། ཕྱག་གཡས་ན་ལ་ཕུག་བསྣམས་པ། གཡོན་ན་རིན་པོ་ཆེའི་ནོར་བུ་བསྣམས་པ། གསུས་པ་ཆེ་བ། རྒྱན་ཐམས་ཅད་ཀྱིས་བརྒྱན་པ། ཟླ་བ་དང་བྱི་བའི་ཁྲི་ལ་བཞུགས་པ་མཁས་པས་བྱའོ། ། དེ་ལ་དཀྱིལ་འཁོར་གྱི་དབུས་སུ་བདག་ལ་ཞལ་ཕྱོགས་པར་བཞག་ལ། བདག་ཉིད་ལོག་འདྲེན་གྱི་གཟུགས་ཀྱིས་ཡང་དག་པར་མཆོད་དོ། །",
    "d666:40": "ཚོགས་ཀྱི་བདག་པོ་ཞལ་གཅིག་ཕྱག་བཞི་པ་སྒྲུབ་པའི་ལེའུ་སྟེ་བཞི་པའོ།། །།",
    "d666:41": "དེ་ནས་དབང་ལྡན་གྱི་མཚམས་སུ་ཚོགས་ཀྱི་བདག་པོ་དཀར་པོ་གླང་པོ་ཆེའི་མགོ་ཅན། བྱི་བ་ལ་ཞོན་པ། མིག་གསུམ་པ། ཟླ་བ་ཕྱེད་པ་འཛིན་པ། གཡས་ན་ལ་ཕུག་དང་དགྲ་སྟ་འཛིན་པ། གཡོན་པ་ཏྲི་ཤཱུལ་དང་ཐོད་པ་འཛིན་པའོ། །",
    "d666:42": "ལག་པ་གཡོན་པ་ཁུ་ཚུར་བཅངས་པའི་མཛུབ་མོ་དང་གུང་མོ་བརྐྱང་ལ་མཛུབ་མོ་བཀུག་ལ་གུང་མོའི་ཚིགས་སུ་བཟུང་བར་བྱ་བ་ནི་ཚོགས་ཀྱི་བདག་པོའི་ཕྱག་རྒྱའོ། །",
    "d666:43": "ཚོགས་ཀྱི་བདག་པོའི་ཕྱག་རྒྱ་བསྟན་པའི་ལེའུ་སྟེ་ལྔ་པའོ།། །།",
    "d666:44": "དེ་ནས་གསང་སྔགས་བསྟན་པ་ནི། །ཨོཾ་ནི་དཔལ་ལྡན་མགོ་ལ་བཞག །",
    "d666:45": "དེ་ནས་ཡིད་བཞིན་ནོར་བུ་ནི། །རིན་པོ་ཆེ་ཡི་ཡི་གེ་བྲི། །སྡེ་ཚན་གསལ་བྱེད་དང་པོ་ཡང་། །དེ་ཡི་རྗེས་སུ་བྲི་བར་བྱ། །",
    "d666:46": "དེ་བཞིན་གཤེགས་པའི་མཚན་དང་པོ། །དབྱངས་ཡིག་བརྒྱད་པས་མཛེས་པར་བྱ། །",
    "d666:47": "གསལ་བྱེད་དང་པོའི་ཡི་གེ་ཡང་། །བློ་ཡིས་བླངས་ཏེ་ཡིད་ཀྱིས་བྲི། །སྡེ་ཚན་གསུམ་པས་གསུམ་པ་ལ། །དང་པོའི་བཞི་པ་སྦྱར་བར་བྱ། །",
    "d666:48": "སྡེ་ཚན་བདུན་པའི་གསུམ་པ་ལ། །གསུམ་པའི་དང་པོས་འོག་ཏུ་སྦྱར། །",
    "d666:49": "ཡིད་བཞིན་ནོར་བུའི་ཡི་གེ་ལ། །བཞི་པའི་ལྔ་པ་ཟླ་ལ་བཞག །",
    "d666:50": "དབྱངས་ཡིག་གསུམ་པས་བརྒྱན་པར་བྱ། །",
    "d666:51": "བཞི་པའི་གསུམ་པས་དེ་ནས་བརྗོད། །",
    "d666:52": "དངོས་གྲུབ་བསྒྲུབ་པའི་ཡི་གེ་ནི། །དེ་ཡི་རྗེས་ལ་བྲི་བར་བྱ། །",
    "d666:53": "གསང་སྔགས་དྲག་པོའི་ཡི་གེ་ཡང་། །ཀུན་གྱི་མཐར་ནི་བརྗོད་པར་བྱ། །",
    "d666:54": "གསང་སྔགས་འདི་ཡི་དངོས་གྲུབ་ནི། །འཇིག་རྟེན་དབང་ཕྱུག་ཉིད་ཀྱིས་བྱིན། །",
    "d666:55": "དགོས་པ་ཐམས་ཅད་གྲུབ་པར་འགྱུར། །",
    "d666:56": "ཚོགས་བདག་ཆེན་པོའི་གསང་སྔགས་བསྟན་པའི་ལེའུ་སྟ་དྲུག་པའོ།། །།",
    "d666:57": "དེ་ནས་གཞན་ཡང་གསུངས་པ་ནི། །དངོས་གྲུབ་ཐམས་ཅད་སྒྲུབ་པ་ཡི། །མཚན་གྱི་གསང་སྔགས་ཞེས་བྱ་བའི། །དངོས་གྲུབ་བསྒྲུབ་པའི་སྙིང་པོ་ནི། །དཔལ་དང་ཡཾ་གི་ཡི་གེ་སྟེ། །",
    "d666:58": "ཨོཾ་ཞེས་བྱ་བ་མགོ་ལ་བཞག །",
    "d666:59": "ཡིད་བཞིན་ནོར་བུའི་ཡི་གེ་ལས། །ཉི་མའི་ཡི་གེ་འོག་ཏུ་སྤྲས། །དབྱངས་ཡིག་དྲུག་པའི་འོག་ཏུ་སྦྱར། །",
    "d666:60": "ཐིག་ལེ་ནཱ་དས་བརྒྱན་པར་བྱ། །",
    "d666:61": "མཚན་གྱི་དང་པོའི་ཡི་གེ་ནི། །དེ་ཡི་རྗེས་ལ་བརྗོད་པར་བྱ། །",
    "d666:62": "བཞི་པའི་ལྔ་པ་དེ་ནས་སྦྱར། ། ལྔ་པའི་དང་པོ་བྲི་བར་བྱ། །",
    "d666:63": "དེ་བཞིན་གཤེགས་པའི་མཚན་དང་པོ། །འབྱུང་བ་རླུང་གི་ཡི་གེ་ཡང་། །དབྱངས་ཡིག་བརྒྱད་པས་བརྒྱན་པ་ནི། །དེ་ཡི་རྗེས་ལ་བྲི་བར་བྱ། །",
    "d666:64": "ཞི་བའི་ཡིག་འབྲུ་གཉིས་པ་ཡང་། །ཀུན་གྱི་མཐར་ནི་བརྗོད་པར་བྱ། །",
    "d666:65": "མཚན་གྱི་གསང་སྔགས་ཞེས་བྱ་བ། །འདོད་པ་ཐམས་ཅད་གྲུབ་བྱེད་ཡིན། །",
    "d666:66": "མཚན་གྱི་གསང་སྔགས་བསྟན་པའི་ལེའུ་སྟེ་བདུན་པའོ།། །།",
    "d666:67": "དེ་ནས་གཞན་ཡང་བཤད་པར་བྱ། །",
    "d666:68": "སྐར་མ་རྒྱལ་གྱི་ཉིན་པར་ནི། །མེ་ཏོག་དཀར་པོའི་རྩ་བ་བླང་། །ཙནྡན་དཀར་པོ་དག་ལས་ནི། །ཚོགས་ཀྱི་བདག་པོའི་སྐུ་གཟུགས་ནི། །མཐེ་བོང་གི་ནི་ཚད་ཙམ་བྱ། །",
    "d666:69": "གསུས་པ་ཆེལ་ཕྱག་བཞི་པ། །མཁས་པས་བྱས་ཏེ་ལེགས་པར་བྱ། །",
    "d666:70": "རིན་པོ་ཆེ་ཡི་སྣོད་དུ་གཞུག །",
    "d666:71": "དཀར་ཆེན་མངོན་པར་གསལ་བའི་ཚེ། །ནང་གི་ཆ་རིས་སྐུ་ཁྲུས་གསོལ། །གསལ་ཞིང་དང་ས་ལ་ཡིད་དུ་འོང་། །",
    "d666:72": "དྲི་དང་ལྡན་པའི་བུམ་པ་ཡི། །མེ་ཏོག་དཀར་པོའི་དྲི་ཡི་ནི། །འདག་ཆལ་ལྡན་པས་ཁྲུས་བྱའོ། །",
    "d666:73": "དེ་ནས་དེ་མ་ཐག་ཏུ་ནི། །དཀྱིལ་འཁོར་དབུས་སུ་སྤྱན་དྲངས་ལ། །མེ་ཏོག་དཀར་པོ་དབུལ་བར་བྱ། །",
    "d666:74": "ལ་ཕུག་ལ་དུར་བཅས་པ་ཡི། །གཏོར་མ་ཆེན་པོའང་དེ་ནས་དབུལ། །སྤོས་དང་དྲི་དང་མར་མེ་དང་། །ལྷ་བཤོས་དག་གིས་མཆོད་པར་བྱ། །",
    "d666:75": "གསང་སྔགས་འདི་ལ་ཕུག་དང་། །ལ་དུ་དང་། ཆུ་དང་། དཀར་གསུམ་གྱི་གཏོར་མས་ཚོགས་ཀྱི་བདག་པོ་ལ་ཕུལ་ཏེ། སྤོས་དང་མེ་ཏོག་རྣམས་ཀྱང་དབུལ། དེ་ལྟར་བྱས་ན་ཕྱིས་བགེགས་བྱེད་པར་མི་ནུས་སོ། །",
    "d666:76": "ཚོགས་ཀྱི་བདག་པོ་མཆོད་པའི་ལེའུ་སྟེ་བརྒྱད་པའོ།། །།",
    "d666:77": "དེ་ནས་ལ་ཕུག་ཅེས་བྱ་བ། །རོ་བརྒྱད་ལྡན་པའི་ཡུངས་མ་སྟེ། །",
    "d666:78": "ཟས་རྣམས་ཀུན་གྱི་མཆོག་ཀྱང་ཡིན། །",
    "d666:79": "རོ་མཆོག་བརྒྱད་དང་ལྡན་པ་ཡིན། །",
    "d666:80": "ཁ་དང་སྐྱུར་དང་བསྐ་བ་དང་། །མངར་དང་ཚ་དང་ལན་ཚྭའི་རོ། །",
    "d666:81": "བཞུན་དང་བྲོ་མཆོག་ཆེ་བ་ཡིན། །",
    "d666:82": "ནད་སྣ་བརྒྱད་ཀྱི་གཉེན་པོ་ཡིན། །",
    "d666:83": "མཁྲིས་པ་བད་ཀན་འདུས་པ་དང་། །ཁྲག་དང་ཆུ་སེར་མ་ཞུ་བ། །རླུང་དང་རིམས་ནད་ཀུན་ལ་ཕན། །",
    "d666:84": "འདི་འདྲ་བྱུང་བའི་ཁ་ཟས་མེད། །",
    "d666:85": "ལྷ་རྣམས་ཀུན་གྱི་མཆུ་ཡང་འགུལ། །དངོས་གྲུབ་འགྲུབ་པའི་ཞལ་ཟས་ཡིན། །",
    "d666:86": "ཚོགས་ཀྱི་བདག་པོ་མཆོད་པར་བྱ། །",
    "d666:87": "ལ་དུ་ཞེས་བྱའི་ཞལ་ཟས་ནི། །མངར་གསུམ་འབྲས་བུ་གསུམ་པ་ནི། །དེ་བཞིན་ཞིམ་པའི་སྤོས་ཀྱིས་བྲན། །བག་ཟན་གྱི་ནི་རིལ་བུ་བྱ། །",
    "d666:88": "འོ་མ་མར་དུ་བཙོས་བྱས་ལ། །ཚོགས་པའི་གཏོར་མ་ཆེན་པོ་བྱ། །",
    "d666:89": "མར་དང་ཁུར་བ་མངར་གསུམ་དང་། །མཐུན་པའི་ཞལ་ཟས་སྣ་ཚོགས་ཀྱིས། །ཚོགས་བདག་ཆེན་པོ་མཆོད་པར་བྱ། །",
    "d666:90": "དེ་ལྟར་ཚོགས་ཀྱི་བདག་པོ་ལའོ། །",
    "d666:91": "ཚོགས་ཀྱི་བདག་པོའི་སྐུ་གཟུགས་ལག་པ་གཡས་པའི་མཐིལ་དུ་བཞུགས་ལ། སེམས་གཞན་དུ་མ་ཡེངས་པར་བཟླས་བརྗོད་འབུམ་བྱས་ལ། ལ་ཕུག་ལ་སྔགས་ཀྱིས་བཏབ་སྟེ་ ཚོགས་ཀྱི་བདག་པོ་མཆོད་ན་ ཚེ་འདི་ཉིད་ལ་འཁོར་ལོས་སྒྱུར་བའི་རྒྱལ་པོ་དང་བསོད་ནམས་མཉམ་པར་འགྱུར་རོ། ། མེ་ཏོག་དཀར་པོས་མཆོད་ན་རྒྱལ་པོ་ལ་སོགས་པས་ཕམ་པར་མི་འགྱུར་རོ། ། རྒྱལ་པོ་ལ་སོགས་པ་སྒྲུབ་པར་འགྱུར་རོ། །",
    "d666:92": "རྒྱལ་པོ་དང་བློན་པོ་ལ་སོགས་པ་བདག་གི་དབང་དུ་འདུས་པར་འགྱུར་རོ། ། དངོས་གྲུབ་སྒྲུབ་པའི་ཞལ་ཟས་བསྟན་པའི་ལེའུ་སྟེ་དགུ་པའོ།། །།",
    "d666:93": "དེ་ནས་ཆོ་ག་བཤད་པར་བྱ། །",
    "d666:94": "དངུལ་ཞེས་བྱ་བའི་རིན་ཆེན་ལ། །ཚོགས་བདག་ཆེན་པོའི་གཟུགས་བྱས་ཏེ། ། ཡིད་བཞིན་ནོར་བུ་ལག་ན་ཐོགས། །",
    "d666:95": "དཀྱིལ་འཁོར་ཆེན་པོ་ཁྲུ་གང་ཚད། །",
    "d666:96": "རིན་ཆེན་མེ་ཏོག་དཀར་པོ་དགྲམ། །གསང་སྔགས་འབུམ་དུ་བཟླས་བྱས་པས། །ནད་དང་སྡུག་བསྔལ་ཞི་བར་འགྱུར། །",
    "d666:97": "ཤེལ་དང་དུང་ལས་ཆོགས་བདག་སྐུ། །མཁས་པས་ཇི་ལྟར་འབྱོར་པར་བྱ། །",
    "d666:98": "སྡུག་བསྔལ་ཐམས་ཅད་སེལ་བར་འགྱུར། །",
    "d666:99": "རིན་ཆེན་ཤེལ་ལས་ཚོགས་བདག་སྐུ། །མཐེ་བོང་གི་ནི་ཚད་ཙམ་བྱ། །",
    "d666:100": "སེ་ན་པྲ་ཞེས་བྱ་བའི་སྔགས། །",
    "d666:101": "རིག་པ་འབུམ་དུ་བཟླས་བྱས་ཏེ། །",
    "d666:102": "ཤེལ་གྱི་སྐུ་དེ་ཁྲུས་བྱས་ནས། །",
    "d666:103": "སྐར་མ་རྒྱལ་གྱི་ཉིན་པར་ནི། །བདག་ཉིད་ཤར་དུ་ཁ་བལྟས་ནས། །",
    "d666:104": "གོས་དཀར་གྱོན་ཏེ་ཁྲུས་བྱས་ལ། །ཐལ་སྦྱར་ཡིད་ཀྱིས་གསོལ་བ་གདབ། །བདག་ལ་གནོད་པའི་བགེགས་གང་ཡིན། །",
    "d666:105": "ཤེལ་གྱི་སྐུ་དེ་མེ་ལོང་སྟེང་། །མི་གཡོ་མི་འགུལ་རབ་ཏུ་བཞུགས། །",
    "d666:106": "བདག་ཉིད་ཁྲུས་བྱས་ཤར་དུ་བལྟ། །",
    "d666:107": "ཛ་ར་པ་དང་རྐུན་མ་ནི། །གང་ཡིན་མེ་ལོང་ནང་དུ་འབྱུང་། །",
    "d666:108": "དེ་ཚེ་དེ་ཡི་ངོས་ཟིན་འགྱུར། །",
    "d666:109": "ཚོགས་བདག་ཤེལ་གྱི་སྐུ་དེ་ནི། །མཆོད་པ་རབ་གསལ་ལེགས་བྱས་ལ། །ནཱ་ག་ཀུ་རུ་ཞེས་བྱ་བའི། །རིག་པ་འབུམ་དུ་བཟླས་བྱས་ན། །",
    "d666:110": "ནད་དང་སྡུག་བསྔལ་གྱིས་ཉེན་ལ། །སྐུ་དེ་བསྟན་པ་ཙམ་གྱིས་ནི། །ནད་རྣམས་ཐམས་ཅད་སོས་པར་འགྱུར། །",
    "d666:111": "སྡུག་བསྔལ་ལས་ཀྱང་ཐར་བར་འགྱུར། །",
    "d666:112": "ཞི་བ་ཆེན་པོའི་ཆོ་ག་ཡིན། །",
    "d666:113": "རིན་ཆེན་གསེར་ཞེས་བྱ་བ་ལ། །ཚོགས་བདག་ཆེན་པོའི་སྐུ་བྱས་ཏེ། །",
    "d666:114": "རྒྱལ་གྱི་ཉིན་པར་མིག་ཕྱེ་ནས། །",
    "d666:115": "མཆོད་པ་རབ་ཏུ་གནས་པ་བྱ། །",
    "d666:116": "བ་ར་ཞེས་བྱའི་གསང་སྔགས་ནི། །འབུམ་ཕྲག་གཅིག་ཏུ་བཟླས་པ་ཡིས། །སྐུ་གཟུགས་བཞུགས་པའི་ཁྱིམ་དེར་ནི། །ནོར་རྣམས་ཐམས་ཅད་འཕེལ་བར་འགྱུར། །",
    "d666:117": "གསེར་གྱི་ཚོགས་བདག་མཐོང་བ་ཡིས། །བགེགས་རྣམས་ཐམས་ཅད་ནོར་འབུལ་འགྱུར། །",
    "d666:118": "བགེགས་ཀྱིས་གནོད་པ་མི་ནུས་ཏེ། །",
    "d666:119": "རྒྱལ་པོ་བཞིན་དུ་མཆོད་པར་འགྱུར། །",
    "d666:120": "གསེར་གྱི་ཚོགས་བདག་གནས་པ་ཡི། །ས་ཕྱོགས་རིན་ཆེན་ཆར་པ་འབེབས། །",
    "d666:121": "གསེར་གྱི་ཚོགས་ཀྱི་བདག་པོ་ལ། །བགེགས་ཀྱི་རྒྱལ་པོ་ཐམས་ཅད་ཀྱིས། །ནོར་རྣམས་ཐམས་ཅད་འབུལ་བ་ཡིན། །",
    "d666:122": "བདག་ཉིད་ཕུན་སུམ་ཚོགས་པར་འགྱུར། །",
    "d666:123": "ཚོགས་ཀྱི་བདག་པོའི་སྐུ་གཟུགས་མཆོག །བྱས་པའི་ཡོན་ཏན་རྣམས་ལ་ནི། །གནོད་སྦྱིན་རྣམས་ཀྱིས་གཏེར་ཁ་འབྱེད། །",
    "d666:124": "སྐྱེ་བ་འདི་དང་ཕྱི་མ་ལ། །རྟག་ཏུ་ཡོ་བྱད་འཕེལ་བར་འགྱུར། །",
    "d666:125": "ནོར་གྱི་བདག་པོ་རྣམ་ཐོས་བུས། །",
    "d666:126": "རྒྱལ་པོ་བཞིན་དུ་བསྙེན་བཀུར་བྱེད། །",
    "d666:127": "དེ་ནས་དཔལ་ལྡན་ཚོགས་བདག་གི། །སྐུ་གཟུགས་དཀྱིལ་འཁོར་གྲུ་བཞིར་གཞག །",
    "d666:128": "བྷ་ག་ཞེས་བྱའི་གསང་སྔགས་ནི། །བཟླས་པ་འབུམ་དུ་བྱས་པ་ཡིས། །ཚེ་དེ་ཉིད་ལ་དངོས་གྲུབ་ནི། །ཅི་འདོད་པ་ནི་སྦྱིན་པར་མཛད། །",
    "d666:129": "ལྷ་ཡི་རྒྱལ་པོའི་མངའ་ཐང་ནི། །སྒྲུབ་པ་པོ་ལ་སྩོལ་བར་མཛད། །",
    "d666:130": "ཤིན་ཏུ་ཐུགས་རྗེའི་བདག་ཉིད་པས། །དཔལ་ལྡན་ཚོགས་བདག་ཐུགས་རྗེ་ཅན། །ཐུགས་རྗེའི་ཕུང་པོས་དབུལ་བ་སྐྱོབ། །",
    "d666:131": "ནོར་ལྷ་ཆེན་པོའི་བདག་ཉིད་འདི། །དར་རམ་རས་ལ་ལེགས་བྲིས་ཏེ། །",
    "d666:132": "རྒྱལ་གྱི་ཉིན་པར་སྤྱན་ཕྱེ་ནས། །",
    "d666:133": "གང་གི་ཁྱིམ་དུ་བཞུགས་པ་དེའི། །ས་ཕྱོགས་དེ་ནི་བཀྲ་ཤིས་འགྱུར། །",
    "d666:134": "ཟངས་ལ་ཚོགས་བདག་ཆེན་པོའི་སྐུ། །མཁས་པས་ལེགས་པར་བཟོ་བྱས་ཏེ། །",
    "d666:135": "ཙནྡན་དམར་པོས་བྱུགས་པའི་སར། །མཎྜལ་སྟེང་དུ་ལེགས་པར་བཞག །",
    "d666:136": "བནྡུ་ཀ་ཡི་མེ་ཏོག་དང་། །པདྨ་རཱ་ག་རིན་ཆེན་དང་། །མེ་ཏོག་དམར་པོ་ཀུན་གྱིས་མཆོད། །",
    "d666:137": "བྲཾ་ཞེས་བྱ་བའི་རིག་སྔགས་བཟླས། །རྒྱལ་པོའི་བུ་མོ་དབང་དུ་འགྱུར། །",
    "d666:138": "ལྷ་ཡི་བུ་མོ་ཀླུ་མོ་དང་། །མིའམ་ཅི་ཡི་བུ་མོ་དང་། །བྲམ་ཟེའི་བུ་མོ་མཚན་ལྡན་མོ། །",
    "d666:139": "མཆོག་ཏུ་གཟུགས་བཟང་གུས་པར་འགྱུར། །",
    "d666:140": "བྲན་མོ་བཞིན་དུ་བསྒོ་བ་ཉན། །",
    "d666:141": "དེ་ལ་སོགས་པ་མཐའ་ཡས་མཆོག །སྐུ་གཟུགས་དག་ལ་བརྟེན་ཏེ་འགྲུབ། །",
    "d666:142": "མཆོད་པ་ཙམ་གྱིས་འབྱུང་བ་ཡིན། །",
    "d666:143": "གཞན་ཡང་ཚོགས་བདག་ཆེན་པོའི་སྐུ། །རིན་པོ་ཆེ་ཡི་རྒྱུ་ལས་བྱས། །",
    "d666:144": "དཀྱིལ་འཁོར་སྟེང་དུ་བཞུགས་བྱས་ཏེ། །",
    "d666:145": "རཏྣ་སིདྡྷི་ཞེས་བྱ་བའི། །རིག་པ་འབུམ་དུ་བཟླས་པར་བྱ། །",
    "d666:146": "ས་འོག་གཏེར་རྣམས་སྟོན་པར་འགྱུར། །",
    "d666:147": "ཅི་དགའ་བར་ཡང་སྤྱོད་པར་འགྱུར། །",
    "d666:148": "ཚོགས་བདག་ཆེན་པོའི་རྒྱུད་ལས་རྒྱལ་པོ་ལ་སོགས་པ་སྒྲུབ་པའི་ལེའུ་སྟེ་བཅུ་པའོ།། །།",
    "d666:149": "དེ་ནས་དཔལ་ལྡན་ཚོགས་བདག་ཆེ། །",
    "d666:150": "དྲག་པོ་ཆེན་པོ་དྲག་ཤུལ་ཅན། །སངས་རྒྱས་ཐུགས་ལ་བྱོལ་སོང་དབུ། །གླང་ཆེན་གཞོན་ནུས་ཞལ་བརྒྱན་པ། །སྤྱན་གསུམ་མཆེ་བ་བཞི་དང་ལྡན། །",
    "d666:151": "ཕྱག་བཞི་ཞབས་བཞི་ཀླུ་ཆེན་ལྡན། །",
    "d666:152": "དང་པོ་མཆེ་བ་དཀར་པོ་འཛིན། །",
    "d666:153": "དེ་འོག་ཞགས་པ་གཡོན་པ་ན། །",
    "d666:154": "དགྲ་སྟ་ཁ་ཊྭཱཾ་རྩེ་གསུམ་འཛིན། །",
    "d666:155": "མཐིང་ག་གསུས་པ་ཆེ་བའོ། །",
    "d666:156": "དེ་ལྟ་བུ་ཡི་ཚོགས་བདག་སྐུ། །ཁར་བ་ཁྲོ་ཆུ་དག་ལ་བྱ། །",
    "d666:157": "རྒྱལ་གྱི་ཉིན་པར་རབ་ཏུ་གནས། །",
    "d666:158": "དེ་ནས་བསྒོམ་ཞིང་བསྒྲུབ་པ་ནི། །དཀྱིལ་འཁོར་བྱ་བའི་ས་བརྟག་པ། །ལེའུ་ཚེ་དགུ་པའི་དཀྱིལ་འཁོར་བྲི། །སྒོ་དང་སྒོ་ཁྱུད་ལྡན་པར་བྱ། །",
    "d666:159": "དབུས་སུ་པདྨ་འདབ་བརྒྱད་ལ། །ཚོགས་བདག་ཆེན་པོས་བསྒྲུབ་བྱ་མནན། །",
    "d666:160": "ཟླ་བའི་གདན་ལ་ཡུམ་དང་ནི། །གཉིས་སུ་མེད་པ་དགོད་པར་བྱ། །",
    "d666:161": "དེའི་མིང་ཨརྩི་ཀ་རི་སྟེ། །",
    "d666:162": "རྡོ་རྗེ་ཕག་གདོང་མ་ཞེས་བྱ། །",
    "d666:163": "དགྲ་སྟ་བནྡ་ལག་ན་ཐོགས། །",
    "d666:164": "ཁྲག་གིས་བཀང་བ་གཙོ་ལ་སྟོབས། །ཤར་གྱི་ལེའུ་ཚེ་སེར་པོ་ལ། ཁམས་གསུམ་གྱི་བདག་པོ་ལ་ཕུག་དང་གྲི་འཛིན་པའོ། །",
    "d666:165": "ལྷོ་ཕྱོགས་ཀྱི་ལེའུ་ཚེ་སྔོན་པོ་ལ། གནོད་སྦྱིན་ཆེན་པོ་རྡོ་རྗེ་མཆུ། མ་ཧེ་དེ་བའི་གདོང་ཅན་དབྱུག་ཏོ་དང་ཐོ་བ་འཛིན་པ་ནག་པོའོ། །",
    "d666:166": "ནུབ་ཕྱོགས་ཀྱི་ལེའུ་ཚེ་དམར་པོ་ལ། གནོད་སྦྱིན་གྱི་རྒྱལ་པོ་སྤྲིན་གྱི་བདག་ཅེས་བྱ་བ་དམར་པོ་ཆུས་གང་བའི་སྤྲིན་ཕུང་དང་ཞགས་པ་འཛིན་པའོ། །",
    "d666:167": "བྱང་ཕྱོགས་ཀྱི་ལེའུ་ཚེ་ལྗང་གུ་ལ་གནོད་སྦྱིན་གྱི་བདག་པོ་ནོར་གནས་ཞེས་བྱ་བ་ལྗང་གུ་དབྱུག་ཐོ་དང་རིན་པོ་ཆེའི་ཁ་ཊྭཱྃ་ག་ཐོགས་པའོ། །",
    "d666:168": "བྱང་ཤར་གྱི་ལེའུ་ཚེ་དཀར་པོ་ལ་བགེགས་ཀྱི་རྒྱལ་པོ་བྱི་ན་ཡ་ཀ་གླང་པོའི་གདོང་མདུང་རྩེ་གསུམ་པ་དང་ལ་ཕུག་ཐོགས་པ་ནག་པོའོ། །",
    "d666:169": "ཤར་ལྷོའི་ལེའུ་ཚེ་དམར་པོ་ལ་ཡི་དགས་ཀྱི་བདག་པོ་དྲག་པོ་ཞེས་བྱ་བ་དགྲ་སྟ་དང་། ཐོད་པའི་རིལ་པོ་ཐོགས་པ་ནག་པོའོ། །",
    "d666:170": "ལྷོ་ནུབ་ཀྱི་ལེའུ་ཚེ་ནག་པོ་ལ་སྤྲིན་གྱི་སྒྲོན་མ་ཞེས་བྱ་བ་སྤྲིན་ཕུང་དང་གློག་གི་འོད་དུ་བཅས་པ་ལག་གཉིས་ཀྱིས་ཐོགས་པའོ། །",
    "d666:171": "བྱང་ནུབ་ཀྱི་ལེའུ་ཚེ་ནག་པོ་ལ་བགེགས་ཀྱི་རྒྱལ་པོ་ལོག་འདྲེན་སྣ་ཡོན་ཞེས་བྱ་བ་གླང་པོ་ཆེའི་སྣ་ཙན་རྩེ་གསུམ་དང་ཐོད་པ་སྐམ་པོའི་ནང་དུ་ལ་ཕུག་དང་ཤས་བཀང་བ་ཐོགས་པའོ། །",
    "d666:172": "ལྷ་རྣམས་ལ་ཀླུའི་རྒྱལ་པོ་བརྒྱད་བཀའ་ཉན་པ་ནི་ཀླུའི་རྒྱལ་པོ་ཨ་བ་ལ་དང་། བ་རུ་ཎ་དང་། དུང་སྐྱོང་དང་། འཇོག་པོ་དང་། རིགས་ལྡན་དང་། མཐའ་ཡས་དང་། པདྨ་དང་། པདྨ་ཆེན་པོ་རྣམས་སོ། །",
    "d666:173": "ཤར་གྱི་སྒོར་རྒྱལ་པོ་ཆེན་པོ་ཡུལ་འཁོར་སྲུང་། ལྷོ་སྒོར་འཕགས་སྐྱེས་པོ། །ནུབ་སྒོར་སྤྱན་མི་བཟང་། བྱང་སྒོར་རྣམ་ཐོས་སྲས་རྣམས་སྐུ་མདོག་དཀར་བ་དང་། སྔོ་བ་དང་། དམར་བ་དང་། ལྗང་གུར་ཤེས་པར་བྱའོ། ། ལག་ན་ནོར་བུ་རིན་པོ་ཆེ་རེ་རེ་རལ་གྲི་རེ་རེའོ། །",
    "d666:174": "འདི་རྣམས་ཀྱི་ཆུང་མ་ནི་མི་བྲིའོ། །འདི་ལྟར་བྲིས་པའི་བྱང་ཤར་དུ་གསད་པའི་དཀྱིལ་འཁོར་གྲུ་གསུམ་ལ། དེའི་ནང་དུ་དགྲ་བོའི་གཟུགས་སྦྲ་ཚིལ་ལས་བྱས་པ་རིད་པ། གནག་པ། སྐྲ་ཟེང་བ། ཏིལ་མར་གྱིས་བྱུགས་ཏེ སྙིང་གར་མིང་རུས་བྲིས་ལ་གཞུག་གོ། ། དེའི་སྐེར་རོ་སྐྲ་ལས་བྱས་པའི་ཐག་པ་གདགས་སོ། །",
    "d666:175": "དེ་ལྟར་བྱས་ནས་ གནོད་སྦྱིན་ཆེན་པོ་བརྒྱད་ལ། ལ་དུ་དང་། ལ་ཕུག་དང་བཅས་པའི་གཏོར་མ་དང་། གནོད་སྦྱིན་ཆེན་པོ་བརྒྱད་ལ་ཉ་ཤ་དང་སྒོག་བཙོང་དུ་སྦྱར་བའི་དམར་གྱི་གཏོར་མ་རེ་རེ་བཤམ། ཀླུ་ཆེན་པོ་བརྒྱད་ལ་གཏོར་མ་ཆེ་བ་གཅིག་དང་། རྒྱལ་པོ་ཆེན་པོ་བཞི་ལ་རེ་རེ་བྱ། གལ་ཏེ་མ་འབྱོར་ན་ཆེ་བ་གཅིག་བྱ། ལྷ་བཤོས་རེ་རེ་དབུལ་བར་བྱའོ། ། གཞན་ཡང་དྲག་པོའི་རྫས་སྤྱང་ཀིའི་ཐོད་པ་དང་། ཟི་ར་ནག་པོ་དང་། སྐེ་ཚེ་དང་། ཡུངས་ཀར་དང་། ཡུངས་ནག་གཞག་པར་བྱའོ། །",
    "d666:176": "ཚོགས་བདག་ཆེན་པོའི་དཀྱིལ་འཁོར་གྱི་ལེའུ་སྟེ་བཅུ་གཅིག་པའོ།། །།",
    "d666:177": "དེ་ནས་གཞན་ཡང་བཤད་པར་བྱ། །",
    "d666:178": "བསྒོམ་པའི་རིམ་པ་འདི་ལྟ་བུ། །མ་བྱུང་འབྱུང་བར་མི་འགྱུར་བས། །ཤིན་ཏུ་གསང་སྟེ་བསྒྲུབ་པར་བྱ། །",
    "d666:179": "ཡ་ཡི་རང་བཞིན་སྟོང་པ་ལས། །སྣང་བ་རང་བཞིན་ཧཱུྃ་དང་ནི། །ཡ་ཚོགས་བརྒྱད་པའི་ས་བོན་འབྱུང་། །",
    "d666:180": "མིང་གི་སྔགས་ལས་རབ་ཏུ་བསྐྱེད། །",
    "d666:181": "ཁྲོ་བོའི་སྤྲུལ་པས་སྟོང་གསུམ་བཀང་། །",
    "d666:182": "འོད་བསྡུས་ཐོག་མའི་ཧཱུཾ་ལ་འདུས། །",
    "d666:183": "ཚོགས་ཀྱི་བདག་པོ་ཆེན་པོ་དེ། །",
    "d666:184": "འགྲོ་བའི་དོན་དུ་བཞེངས་པ་ནི། །ཁྲོ་བོ་ཆེན་པོ་འཇིགས་པའི་གཟུགས། །ཡུམ་དང་འཁོར་ཡང་རྫོགས་པར་བསྐྱེད། །",
    "d666:185": "ཕུཿབརྒྱད་ཀླུ་བརྒྱད་སེམས་དཔའི་ཚུལ། །རྒྱལ་པོ་དེ་བཞིན་ཁྲོ་བོའི་ཚུལ། །ཡི་གེ་བྷཻ་དང་མིང་ལས་བསྐྱེད། །",
    "d666:186": "འོད་ཟེར་ཚུལ་གྱིས་སྤྱན་དྲངས་ལ། །ཛཿདང་ཛཿཡི་འཕྲོ་འདུ་དང་། །ཧཱུྃ་དང་ཧཱུཾ་གི་གསང་སྔགས་དང་། །ལྕགས་ཀྱུའི་ཕྱག་རྒྱས་སྤྱན་དྲངས་ལ། །ནུས་པའི་མཐུ་དང་ལྡན་པར་བྱ། །",
    "d666:187": "མཆོད་དང་གཏོར་མ་བསྟབ་པར་བྱ། །",
    "d666:188": "བ་ལིཾ་གྲྀཧྞ་ཁ་ཡི་སྔགས། ། སྙིང་པོ་དག་དང་སྤེལ་བར་བྱ། །",
    "d666:189": "བསྒོམ་པའི་རིམ་པའི་ལེའུ་སྟེ་བཅུ་གཉིས་པའོ།། །།",
    "d666:190": "དེ་ནས་གསང་སྔགས་རྒྱལ་པོ་ནི། །དབྱངས་ཡིག་གསལ་བྱེད་དག་ལ་བཏུ། །དང་པོ་བཞི་པ་དེ་རྗེས་བྲི། །བཞི་པའི་ལྔ་པ་གྲལ་ལ་བཞག །",
    "d666:191": "ཡིད་བཞིན་ནོར་བུ་ཡི་གེ་དང་། དེ་ཡི་རྗེས་སུ་བསམ་པར་བྱ། །",
    "d666:192": "དེ་བཞིན་གཤེགས་པའི་མཚན་དང་པོ། །དེ་ཡི་རྗེས་སུ་བརྗོད་པར་བྱ། །",
    "d666:193": "དེ་ལ་དབྱངས་ཡིག་བརྒྱད་པས་བརྒྱན། །",
    "d666:194": "དགུག་པའི་ཡི་གེ་དག་དང་ནི། །འབྱུང་བ་རླུང་གི་ཡི་གེ་ལ། །དབྱངས་ཡིག་གཉིས་པ་སྦྱར་བ་ཡིས། །རྒྱལ་བའི་རྒྱལ་པོ་ཞེས་བྱའོ། །",
    "d666:195": "ཡིད་བཞིན་ནོར་བུའི་ཡི་གེ་ལ། །དབྱངས་ཡིག་གཉིས་པ་སྦྱར་བར་བྱ། །",
    "d666:196": "ཉི་མ་ཡི་ནི་ཡི་གེ་དང་། །བཞི་པའི་ལྔ་པ་དེ་ནས་བྲི། །དེ་ལ་དབྱངས་ཡིག་བརྒྱད་པས་བརྒྱན། །",
    "d666:197": "གཉིས་པའི་བཞི་པའི་ཡི་གེ་ལ། །དབྱངས་ཡིག་བཞི་པས་བརྒྱན་པར་བྱ། །",
    "d666:198": "ལྔ་པའི་བཞི་པའི་ཡི་གེ་ལ། །གསུམ་པའི་ལྔ་པ་སྦྱར་བར་བྱ། །",
    "d666:199": "བཞི་པའི་དང་པོའི་ཡི་གེ་ལས། །དབྱངས་ཡིག་བཞི་པས་བརྒྱན་པར་བྱ། །",
    "d666:200": "འབྱུང་བ་རླུང་གི་ཡི་གེ་ལ། །དེ་ཡི་འོག་ཏུ་སྦྱར་བར་བྱ། །",
    "d666:201": "རིན་པོ་ཆེ་ཡི་ཡི་གེ་དང་། །ལས་ཀྱི་ཡི་གེ་མཐའ་མར་བརྗོད། །",
    "d666:202": "ཚོགས་བདག་ཆེན་པོའི་རྩ་བའི་སྔགས། །",
    "d666:203": "འདོད་པ་ཐམས་ཅད་སྒྲུབ་བྱེད་ཡིན། །",
    "d666:204": "གནོད་སྦྱིན་བརྒྱད་ཀྱིས་གསང་སྔགས་ནི། །ཡ་ཞེས་བྱ་བ་ལ་སོགས་པ། །ཡ་ཡ་ཞེས་བྱ་ལ་སོགས་དང་། །བྃ་ཞེས་བྱ་བ་ལ་སོགས་དང་། །བི་ན་ཡ་ཀ་ཞེས་བྱ་བ་ལ་སོགས་པ་དང་། བྷཻ་ར་ཞེས་བྱ་བ་ལ་སོགས་པ་དང་། །ཧཱུྃ་ཞེས་བྱ་བ་དང་། ཏྲི་ཞེས་བྱ་བ་སྟེ་བརྒྱད་ལ་ཧཱུཾ་དང་ཕཊ་ཀྱིས་མཐའ་བརྟེན་པས་སྲོག་གི་སྙིང་པོའོ། །",
    "d666:205": "ཀླུ་ཡི་སྔགས་ནི་མིང་ལས་བསྐྱེད། །",
    "d666:206": "ཨོྃ་དང་ཧཱུྃ་ཕཊ་ལྡན་པར་བྱ། །",
    "d666:207": "རྒྱལ་པོ་ཆེན་པོའི་གསང་སྔགས་ནི། །བྷཻ་ཞེས་བྱ་བ་དང་། བྷི་ཞེས་བྱ་བ་དང་། བྷ་ཞེས་བྱ་བའི་སྔགས་སུ་འགྱུར་རོ། །",
    "d666:208": "གནོད་སྦྱིན་ཆེན་པོ་རྣམས་ཀྱི་གསང་སྔགས་འབྱུང་བའི་ལེའུ་སྟེ་བཅུ་གསུམ་པའོ།། །།",
    "d666:209": "བསྒྲུབ་བྱ་དགུག་པའི་སྔགས་ནི། །ཡ་དང་ཀྵ་དང་ཀ་དང་ལ། །མ་དང་ཧཱ་དང་བྷཻ་ར་བྷེ། །ཏྲི་དང་རཱ་དང་ཙ་དག་གིས། །ཤ་ཏྲཱུྃ་ནྲྀ་ཛ་ནི་ཛ་ཞེས། །ལན་གྲངས་མང་དུ་བཟླས་པར་བྱའོ། །",
    "d666:210": "གནོད་སྦྱིན་བརྒྱད་ཀྱི་གསང་སྔགས་ཀྱིས། །དགྲ་བོའི་བླ་རྡོ་བླང་བར་བྱ། །",
    "d666:211": "དེ་ནས་བླ་རྡོ་བསྲེག་པར་བྱ། །",
    "d666:212": "དེ་ནས་བླ་རྡོ་མནན་པར་བྱ། །",
    "d666:213": "སྦད་པའི་གསང་སྔགས་རྒྱལ་པོ་ནི། །ཡ་དང་ཀྵ་དང་ཀ་དང་ལ། །མ་དང་ཧཱ་དང་བྷེ་ར་བྷེ། །དེ་བ་ཡཀྵ་ཡ་མ་དང་། ནཱ་ག་ནཱ་ག་ཐུམ་རིལ་ཡང་། །ཆེ་གེ་དགྲ་བོ་ཐུམ་རིལ་ཡ། །ཡ་ཡ་ཞེས་ཀྱང་བསྐུལ་བར་བྱ། །",
    "d666:214": "དེ་ནས་བསད་པའི་གསང་སྔགས་ནི། །མ་དང་ཧཱ་དང་བྷེ་ར་བྷེ། །ཡཀྵ་ལ་སོགས་གོང་དང་མཐུན། །",
    "d666:215": "དེ་རྗེས་འདི་སྐད་བརྗོད་པར་བྱ། །",
    "d666:216": "ནཱ་ཤ་ཡ་ན་ལན་གཉིས་སོ། །",
    "d666:217": "ཤ་ཡ་ཙཱུར་ན་ཙཱུ་ར་ཡ། །ནཱ་ག་ཡ་ཀ་ཏྲ་མཱ་ར་ཡ། །ཧ་ན་ཙ་ཀྲ་རིམ་པ་གཉིས། །བི་ན་ཡ་ག་ཛ་བྷིནྡྷི། །ཏནྟ་ཀ་ར་དེ་ནས་བརྗོད། །",
    "d666:218": "ཆེ་གེ་མོ་མཱ་ར་ཡ་ཞེས་པ། །ཧཱུྃ་ཧཱུྃ་ཕཊ་ཕཊ། དྲག་པོས་བསད་པ་སྔགས་ཀྱི་ལེའུ་སྟེ་བཅུ་བཞི་པའོ།། །།",
    "d666:219": "དེ་ནས་ཡང་ནི་དྲག་པོའི་སྔགས། །",
    "d666:220": "ཨོཾ་བྷི་གྷཱ་བྷི་བ་བྷྱི་སརྦ་བིདྱཱ་བི་ཤ་ཡ་ཧྲྀ་ད་མི་ཏ་ཡ། ཧཱུྃ་ཙི་ཏུ་བ་ཏུ། ཏྲྃ་པྲ་མ་རུ་ཏཱ་ཡ། ཧ་ན་ཧ་ན།",
    "d666:221": "གྲྀཧྞ་གྲྀཧྞ། པ་ཙ་པ་ཙ། བྷྲཧྨ་བྷྲཧྨ། བྷྲཧྨ་ཡ་བྷྲཧྨ་ཡ། ཧཱུྃ་ཧཱུྃ་ཕཊ་ཕཊ། ཁར་པའམ་ཡང་ན་ཟངས་དག་ལ། །ལོག་འདྲེན་ཆེན་པོའི་གཟུགས་བྱས་ལ། །དེ་ལ་རབ་གནས་མཆོད་པ་བྱ། །",
    "d666:222": "དགྲ་བོའི་གཟུགས་བརྙན་འོག་ཏུ་གཞུག །",
    "d666:223": "ཚོགས་ཀྱི་བདག་པོའི་སྟན་དུ་བྱ། །",
    "d666:224": "དགྲ་དེ་འཆི་བར་ཐེ་ཚོམ་མེད། །",
    "d666:225": "ཡང་ནི་སྦྱོར་བའི་ལས་རྣམས་ནི། །དགོས་པ་ཆེན་པོ་འདི་གསུངས་སོ། །",
    "d666:226": "ཚོགས་ཀྱི་བདག་པོའི་སྐུ་ཙནྡན་ནམ་རིན་པོ་ཆེ་ལས་བྱས་ཏེ།",
    "d666:227": "རབ་གནས་བྱས་ལ་མཆོད་པ་ཕུལ་ཏེ། ། སེར་བ་གཅོད་པར་འདོད་ན་སེར་བའི་བདག་པོ་རླུང་ལྷ་དང་ཆུ་ལྷའི་གཟུགས་བྱས་ཏེ། སྤྲིན་གྱི་གཟུགས་དང་། འབྲུག་གི་གཟུགས་བྱས་ལ་ཚོགས་ཀྱི་བདག་པོའི་གདན་དུ་མནན་པར་བྱའོ། ། བདག་ཉིད་ཀྱིས་ཀྱང་མནན་པར་བསམས་ཏེ། ཧི་ཀ་ཙནྡྲ་ཞེས་བྱ་བ་ལ་སོགས་པའི་སྔགས་བཟླས་ན་སེར་བ་ཆོད་པར་འགྱུར་རོ། །",
    "d666:228": "ཡུངས་ཀར་ཡང་ཕྱོགས་སུ་གཏོར་བར་བྱའོ། ། སེར་བ་འབེབས་པར་འདོད་ན། ཆུ་དང་སྤྲིན་གྱི་མཎྜལ་མངོན་པར་བྲིས་ཏེ། ཚོགས་བདག་ཆེན་པོ་འབྲུག་ལ་ཞོན་པར་བསྒོམས་ཏེ། བདག་གི་བཀའ་ཉན་པར་བསམ།",
    "d666:229": "གསང་བའི་རྫས་སུ་བྱ་བ་ལ། ལྕགས་ཕྱེ་གསེར་ཕྱེ་ཟངས་ཕྱེ་རྣམས། གནམ་ཆུ་འབྲས་ཀྱི་ཕྱེ་སྦྲུས་ཏེ།",
    "d666:230": "སེར་བ་ཅི་ཙམ་འདོད་པའི་དབྱིབས་ཆེ་ཆུང་བྱས་ལ་ཁྲག་གིས་བྱུགས་ལ་བདུན་དང་བདུན་ཕྲག་མང་དུ་བྱས་ཏེ། ཀ་ཀ་ཀི་ཀུའི་ཐོད་པའི་ནང་དུ་བླུགས་ཏེ། སྤྲིན་གྱིས་དགབ་པར་བྱའོ། ། ཀ་ཀའི་ཐོད་པ་ལ་སྔགས་བྲི་བར་བྱའོ། ། རིམ་པ་གསུམ་མོ། ། རྟེན་གྱི་རྫས་ཀྱང་གཞག་པར་བྱའོ། །",
    "d666:231": "བྲུྃ་བྲུྃ་བྷ་རི་ཤ་ཡ་ཞེས་བྱ་བ་ལ་སོགས་པའི་སྔགས་འབུམ་ཕྲག་གཅིག་བཟླས་པ་དང་། ཀ་ཀའི་ཐོད་པའི་ནང་དུ་སེར་བ་དངོས་སུ་གསུམ་མམ་བདུན་ནམ་བརྒྱད་བྱུང་བ་དང་། དགྲའི་ཡུལ་དུ་བསྐྱལ་ཏེ་སྔགས་བཟློག་ནས་ བཟླས་པས་འབབ་བོ། །གལ་ཏེ་མ་བབས་ན་ ཚོགས་ཀྱི་བདག་པོའི་སྐུ་དབུ་ཐུར་ལ་བསྟན། ཞབས་གྱེན་ལ་བསྟན་པ་དགྲ་བོའི་ཡུལ་གྱི་དབུས་སུ་སྦའོ། ། མོད་ལ་སེར་བ་འབབ་སྟེ། བྱ་ཀ་ལནྡ་ཀའི་སྒོ་ང་ཙམ་དང་། དབྱིབས་དམར་པོ་ཤ་བའི་རླིག་འབྲས་ཙམ་པ་ཡུན་རིང་པོར་འབབ་བོ། །ལོ་ཏོག་འཇོམས་པའི་སྔགས་ནི་བྲུྃ་ཞེས་པས་བྱའོ། །",
    "d666:232": "དེ་གཅོད་པར་འདོད་ན་ནི་སྐུ་དང་ཀ་ཀི་མ་ཕྱུང་བ་ལ་ཉི་མ་རེ་རེར་འབབ་པས་དབྱུང་བར་བྱའོ། ། ཚོགས་ཀྱི་བདག་པོའི་སྐུ་ལ་མཆོད་པ་འབུལ་བར་བྱའོ། ། གསང་སྔགས་ཀྱང་དབུ་ཞབས་མ་ནོར་བར་རིན་པོ་ཆེའི་ཕྲེང་བ་བརྒྱ་རྩ་བརྒྱད་བཟླས་པར་བྱའོ། ། སྐུ་ཁྲུས་གསོལ་བ་བདག་གི་དཀྱིལ་འཁོར་གྱི་དབུས་སུ་བཞུགས་སུ་གསོལ་ལོ། །",
    "d666:233": "དེ་ལ་སོགས་པ་མཐའ་ཡས་མཆོག །ཚོགས་ཀྱི་བདག་པོས་བྱེད་པ་ཡིན། །",
    "d666:234": "ཕྱག་འཚལ་དཔལ་ལྡན་ཚོགས་བདག་དགྱེས། །ཕྱག་འཚལ་དཔལ་ལྡན་ཚོགས་བདག་ཆེ། །",
    "d666:235": "ཕྱག་འཚལ་དཔལ་ལྡན་ཞི་ཁྲོ་ཆེ། །",
    "d666:236": "དངོས་གྲུབ་འབྱུང་བའི་རྒྱུད་ལས་ཚོགས་ཀྱི་བདག་པོ་ལ་བསྟོད་པའི་ལེའུ་སྟེ་བཅྭ་ལྔ་པའོ།། །།",
    "d666:237": "ཚོགས་ཀྱི་བདག་པོའི་རྒྱུད་དངོས་གྲུབ་འབྱུང་བ་ཞེས་བྱ་བ་རྫོགས་སོ།། །།",
    "d666:238": "རྒྱ་གར་གྱི་མཁན་པོ་དཱི་པཾ་ཀ་ར་ཤྲཱིཛྙཱ་ནས།",
    "d666:239": "འཇིག་རྟེན་དངོས་གྲུབ་ཆེད་དུ་ནི། །རྒྱ་གར་ཡུལ་ནས་བསྣམས་ནས་བྱོན། །",
    "d666:240": "རྒྱལ་བའི་འབྱུང་གནས་གཏད་པ་ཡིན། །",
    "d666:241": "།རྒྱུད་འདི་བསམ་པ་འགྲུབ་པ་ཡིན།། །།"
}